{
  "name": "discord",
  "version": "1.0.0",
  "main": "build/app.js",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "build": "ts-node --files config/deleteBuild.ts && tsc --project tsconfig.json && vite build",
    "prod": "yarn build && yarn start",
    "dev": "cross-env NODE_ENV=dev yarn prod",
    "dev:watch": "cross-env NODE_ENV=dev nodemon --watch 'src/core/**/*' --watch 'src/app.ts' -e ts,tsx --exec 'ts-node --files src/app.ts'",
    "lint": "prettier --write \"src/**/*\""
  },
  "_moduleAliases": {
    "@": "./build"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-emoji": "^2.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.15.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.47",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.7",
    "sass": "^1.59.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "types-tsconfig": "^2.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "@vueuse/core": "^9.13.0",
    "bcrypt": "^5.1.0",
    "bootstrap": "^5.2.3",
    "discord.js": "^14.8.0",
    "discord.js-user-account": "^13.3.0",
    "gemoji": "^8.0.0",
    "jsonwebtoken": "^9.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-emoji": "^2.0.2",
    "mime-types": "^2.1.35",
    "mkdirp": "^2.1.5",
    "pinia": "^2.0.0-rc.10",
    "socket.io": "^4.6.1",
    "vue": "^3.2.36",
    "vue-router": "^4.0.13"
  }
}
