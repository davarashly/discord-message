{
  "name": "discord",
  "version": "1.0.0",
  "main": "build/app.js",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "build": "ts-node --files config/deleteBuild.ts && tsc --project tsconfig.json",
    "prod": "pnpm build && pnpm start",
    "dev": "cross-env NODE_ENV=dev pnpm prod",
    "dev:watch": "cross-env NODE_ENV=dev nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node --files src/app.ts'",
    "lint": "prettier --write \"src/**/*.ts\""
  },
  "_moduleAliases": {
    "@": "./build"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "types-tsconfig": "^1.2.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "discord.js": "^14.3.0",
    "discord.js-user-account": "^13.3.0"
  }
}
